{
  "name": "nfdl",
  "type": "module",
  "version": "1.0.7",
  "packageManager": "pnpm@10.16.1",
  "description": "NFDL is a handy CLI tool that simplifies the process of downloading and installing popular Nerd Fonts. Nerd Fonts are a collection of over 50 patched fonts with additional icons and glyphs, designed for developers and terminal enthusiasts.",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/nfdl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/nfdl.git"
  },
  "bugs": "https://github.com/rubiin/nfdl/issues",
  "bin": {
    "nfdl": "bin/nfdl.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepublish": "npm run build",
    "prerelease": "npm run build",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "release": "bumpp && clean-publish",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "enquirer": "^2.4.1",
    "got": "^14.4.8",
    "helper-fns": "^2.9.3",
    "is-online": "^12.0.1",
    "unzipper": "^0.12.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.5.0",
    "@types/unzipper": "^0.10.11",
    "@types/yargs": "^17.0.33",
    "bumpp": "^10.2.3",
    "clean-publish": "^5.2.2",
    "jest": "29.7.0",
    "oxlint": "^1.15.0",
    "ts-jest": "29.1.1",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 30000,
    "rootDir": "test",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ]
  }
}
